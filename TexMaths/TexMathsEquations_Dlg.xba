&apos; Invoked when a window is resized
Sub MainWindowListener_windowResized(ev)
      
	&apos; Inner dialog size
	Dim aSize as Variant
	aSize = ev.Source.getSize()

	&apos; Child window index is 0 for OpenOffice and 2 for LibreOffice
	Dim i as Integer
	If ev.Source.Windows(0).isVisible Then i = 0 Else i = 2

	&apos; Workaround for a resizing issue with gtk3
	count = count + 1 &apos; Counts the number of calls to this function
	If count = 2 Then &apos; Don&apos;t resize if 3rd call and symbol icons not yet displayed
		If oDlgMain.getControl(&quot;General1&quot;).Model.ImageURL &lt;&gt; &quot;&quot; Then
			ev.Source.Windows(i).setPosSize(0, 0, aSize.Width, aSize.Height, com.sun.star.awt.PosSize.SIZE)
		End If
	
	Else &apos; Resize in all other cases
		ev.Source.Windows(i).setPosSize(0, 0, aSize.Width, aSize.Height, com.sun.star.awt.PosSize.SIZE)
	End If

End Sub


