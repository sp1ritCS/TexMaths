&apos;	 Macros used to create and manage the TexMaths Equation dialog

&apos; Force variable declaration
Option Explicit

Private oDlgBaseMain as Variant
Private oDlgMain as Variant

&apos; Conversion factors map appFonts -&gt; pixels
Private convX as Double
Private convY as Double

&apos; Counter used in gtk3 resizing workaround
Private count as Integer


&apos; Create and display dialog
Sub TexMathsDialog( sEqSize as String, sEqType as String, sEqCode as String, sEqFormat as String, sEqDPI as String, sEqTransp as String, sEqName as String)

	&apos; Counter for gtk3 resizing workaround
	count = 0
	
	&apos; Parent window
	Dim oParent as Variant
	oParent = ThisComponent.getCurrentController().getFrame().getContainerWindow()

	&apos; Create base outer window as modal dialog
	oDlgBaseMain = CreateBaseDialog(oParent)

	&apos; Set window title
	oDlgBaseMain.setTitle(  _(&quot;TexMaths Equations&quot;) )

	&apos; Embed inner dialog created with the dialog editor
	Dim sURL as String
	sURL = &quot;vnd.sun.star.script:TexMaths.TexMathsEquations_GUI?location=application&quot;
	oDlgMain = CreateInnerDialog(oDlgBaseMain, sURL, sEqSize, sEqType, sEqCode, sEqFormat, sEqDPI, sEqTransp, sEqName)

 	&apos; Read registry
	Dim oSystemInfo as Variant
	oSystemInfo = GetConfigAccess(&quot;/ooo.ext.texmaths.Registry/SystemInfo&quot;, TRUE)
	
	&apos; Get symbol theme
	glb_sSymbolColor = oSystemInfo.SymbolColor	

	Dim aRect as New com.sun.star.awt.Rectangle
	aRect = CreateUnoStruct(&quot;com.sun.star.awt.Rectangle&quot;)

	&apos; Compute conversion factors between map appFonts and pixels
	aRect = oDlgMain.getPosSize()
	convX = aRect.Width / oDlgMain.Model.Width 
	convY = aRect.Height / oDlgMain.Model.Height

	&apos; Convert min width and height to pixels
	Dim minW as Integer, minH as Integer
	minW = DLG_MIN_WIDTH * convX
	minH = DLG_MIN_HEIGHT * convY

	&apos; Set outer window initial position and size
	If oSystemInfo.PositionX &lt;&gt; &quot;&quot; Then
		aRect.X = oSystemInfo.PositionX
	Else
		aRect.X = -1
	End If
		
	If oSystemInfo.PositionY &lt;&gt; &quot;&quot; Then
		aRect.Y = oSystemInfo.PositionY
	Else
		aRect.Y = -1
	End If

	If oSystemInfo.Width = &quot;&quot; Or Val(oSystemInfo.Width) &lt; minW Then
		aRect.Width = minW
	Else
		aRect.Width = oSystemInfo.Width
	End If

	If oSystemInfo.Height = &quot;&quot; Or Val(oSystemInfo.Height) &lt; minH Then
		aRect.Height = minH
	Else
		aRect.Height = oSystemInfo.Height
	End If

	If aRect.X &gt;= 0 And aRect.Y &gt;= 0 Then
		oDlgBaseMain.setPosSize(aRect.X, aRect.Y, 0, 0, com.sun.star.awt.PosSize.POS)
	End If
	
	If aRect.Width &gt; 0 And aRect.Height &gt; 0 Then
		oDlgBaseMain.setPosSize(0, 0, aRect.Width, aRect.Height, com.sun.star.awt.PosSize.SIZE)
	End If
 
	&apos; Show the modal dialog, has to be done by the outer dialog
	If oDlgBaseMain.execute() &gt; 0 Then
		MsgBox _(&quot;Error: dialog box can&apos;t be created...&quot;)
	End If

	&apos; Get outer window position and size
	aRect = oDlgBaseMain.GetPosSize()

	&apos; Delete dialog
	oDlgBaseMain.dispose()

	&apos; Save outer window position and size to the registry
	oSystemInfo.PositionX = aRect.X
	oSystemInfo.PositionY = aRect.Y

	&apos; Dialog is too narrow
	If aRect.Width &lt;  minW Then
		aRect.Width = minW	
	End If
		
	&apos; Dialog is too short
	If aRect.Height &lt;  minH Then
		aRect.Height = minH
	End If

	oSystemInfo.Width = aRect.Width
	oSystemInfo.Height = aRect.Height
	
	&apos; Apply changes
	oSystemInfo.commitChanges()

End Sub



&apos; Create new outer base dialog
&apos; oParent: parent window peer
&apos; Return dialog window
Function CreateBaseDialog(oParent as Variant) as Variant
     
	Dim oToolkit as Variant
	oToolkit = oParent.getToolkit()
	
	Dim WA as Variant, aRect as Variant, oDesc as Variant
	WA = com.sun.star.awt.WindowAttribute
	aRect = CreateUnoStruct(&quot;com.sun.star.awt.Rectangle&quot;)
	oDesc = CreateUnoStruct(&quot;com.sun.star.awt.WindowDescriptor&quot;)

	With oDesc
    	.Type = com.sun.star.awt.WindowClass.SIMPLE
        .WindowServiceName = &quot;dialog&quot;
        .Parent = oParent
        .ParentIndex = -1
        .Bounds = aRect
        .WindowAttributes = WA.CLOSEABLE OR WA.MOVEABLE OR WA.SIZEABLE OR WA.BORDER
	End With

	Dim oDialog as Variant
	oDialog = oToolkit.createWindow(oDesc)

	&apos; Create listeners
	oDialog.addTopWindowListener(CreateUnoListener(&quot;MainWindowListener_&quot;, &quot;com.sun.star.awt.XTopWindowListener&quot;))
	oDialog.addWindowListener(CreateUnoListener(&quot;MainWindowListener_&quot;, &quot;com.sun.star.awt.XWindowListener&quot;))

	CreateBaseDialog = oDialog

End Function


&apos; Create inner (child) dialog embedded
&apos; oParent: parent window
&apos; sURL: dialog URL, without toolbar
&apos; Return dialog window
Function CreateInnerDialog(oParent as Variant, sURL as String, sEqSize as String, sEqType as String, sEqCode as String, sEqFormat as String, sEqDPI as String, sEqTransp as String, sEqName as String) as Variant

	&apos; Read config
	Dim oSystemInfo as Variant
	oSystemInfo = GetConfigAccess( &quot;/ooo.ext.texmaths.Registry/SystemInfo&quot;, TRUE)	
	
	Dim oDP as Variant, oChildDialog as Variant
	oDP = CreateUnoService(&quot;com.sun.star.awt.ContainerWindowProvider&quot;)
	oChildDialog = oDP.createContainerWindow(sURL, &quot;&quot;, oParent, nothing)
	oChildDialog.setVisible(True)

	&apos; Dialog strings for translation
	oChildDialog.getControl(&quot;EquationNameLabel&quot;).Model.Label = _(&quot;Name&quot;)
	oChildDialog.getControl(&quot;EquationNameText&quot;).Model.HelpText = _(&quot;Enter equation name here (can be left blank)&quot;)
	oChildDialog.getControl(&quot;EquationNameText&quot;).setText(sEqName)
	oChildDialog.getControl(&quot;ImageformatFrame&quot;).Model.Label = _(&quot;Image Format&quot;)
	oChildDialog.getControl(&quot;EquationtypeFrame&quot;).Model.Label = _(&quot;Equation Type&quot;)
	oChildDialog.getControl(&quot;FontsizeFrame&quot;).Model.Label = _(&quot;Font Size&quot;)
	oChildDialog.getControl(&quot;Format&quot;).Model.HelpText = _(&quot;Set image format&quot;)
	oChildDialog.getControl(&quot;GraphicDPI&quot;).Model.HelpText = _(&quot;Set image resolution&quot;)
	oChildDialog.getControl(&quot;Transparency&quot;).Model.Label = _(&quot;Transparency&quot;)
	oChildDialog.getControl(&quot;Transparency&quot;).Model.HelpText = _(&quot;Toggle image transparency&quot;)
	oChildDialog.getControl(&quot;Size&quot;).Model.HelpText = _(&quot;Set font size&quot;)
	oChildDialog.getControl(&quot;TypeDisplay&quot;).Model.HelpText = _(&quot;Inline Display equation&quot;)
	oChildDialog.getControl(&quot;TypeInline&quot;).Model.HelpText = _(&quot;Inline equation&quot;)
	oChildDialog.getControl(&quot;TypeLatex&quot;).Model.HelpText = _(&quot;Regular LaTeX code&quot;)
	oChildDialog.getControl(&quot;HelpButton&quot;).Model.Label = _(&quot;Help...&quot;)
	oChildDialog.getControl(&quot;HelpButton&quot;).Model.HelpText = _(&quot;Display help text&quot;)	
	oChildDialog.getControl(&quot;PreferencesButton&quot;).Model.Label = _(&quot;Preferences...&quot;)
	oChildDialog.getControl(&quot;PreferencesButton&quot;).Model.HelpText = _(&quot;Set equation preferences&quot;)	
	oChildDialog.getControl(&quot;PreambleButton&quot;).Model.Label = _(&quot;Preamble...&quot;)
	oChildDialog.getControl(&quot;PreambleButton&quot;).Model.HelpText = _(&quot;Set preamble&quot;)	

	If oSystemInfo.compiler = &quot;xelatex&quot; Then
		oChildDialog.getControl(&quot;LatexButton&quot;).Model.Label = &quot;Xe~LaTeX&quot;
		oChildDialog.getControl(&quot;LatexButton&quot;).Model.HelpText = _(&quot;Generate XeLaTeX equation&quot;)
	Else
		oChildDialog.getControl(&quot;LatexButton&quot;).Model.Label = &quot;~LaTeX&quot;
		oChildDialog.getControl(&quot;LatexButton&quot;).Model.HelpText = _(&quot;Generate LaTeX equation&quot;)
	End If	

	oChildDialog.getControl(&quot;General&quot;).Model.Label = _(&quot;General&quot;)
	oChildDialog.getControl(&quot;Arrays&quot;).Model.Label = _(&quot;Arrays&quot;)
	oChildDialog.getControl(&quot;Alphabets&quot;).Model.Label = _(&quot;Alphabets&quot;)
	oChildDialog.getControl(&quot;Arrows&quot;).Model.Label = _(&quot;Arrows&quot;)
	oChildDialog.getControl(&quot;Functions&quot;).Model.Label = _(&quot;Functions&quot;)
	oChildDialog.getControl(&quot;Relations&quot;).Model.Label = _(&quot;Relations&quot;)
	oChildDialog.getControl(&quot;Operations&quot;).Model.Label = _(&quot;Operations&quot;)
	oChildDialog.getControl(&quot;Misc&quot;).Model.Label = _(&quot;Misc&quot;)

	&apos; Define list values 
	oChildDialog.getControl(&quot;Format&quot;).addItems(Array(&quot;png&quot;,&quot;svg&quot;),0)
	oChildDialog.getControl(&quot;GraphicDPI&quot;).addItems(Array(&quot;72&quot;,&quot;96&quot;,&quot;120&quot;,&quot;150&quot;,&quot;200&quot;,&quot;300&quot;,&quot;600&quot;,&quot;1200&quot;),0)
	oChildDialog.getControl(&quot;Size&quot;).addItems(Array(&quot;6&quot;,&quot;7&quot;,&quot;8&quot;,&quot;9&quot;,&quot;10&quot;,&quot;10.5&quot;,&quot;11&quot;,&quot;12&quot;,&quot;13&quot;,&quot;14&quot;,&quot;15&quot;,&quot;16&quot;,&quot;18&quot;,&quot;20&quot;,&quot;22&quot;,&quot;24&quot;,&quot;26&quot;,&quot;28&quot;,&quot;32&quot;,&quot;36&quot;,&quot;40&quot;,&quot;44&quot;,&quot;48&quot;,&quot;54&quot;,&quot;60&quot;,&quot;66&quot;,&quot;72&quot;,&quot;80&quot;,&quot;88&quot;,&quot;96&quot;),0)

	&apos; Set the actual values
	oChildDialog.getControl(&quot;Size&quot;).setText(sEqSize)
	If sEqType = &quot;display&quot; Then oChildDialog.getControl(&quot;TypeDisplay&quot;).setState(1)
	If sEqType = &quot;inline&quot; Then oChildDialog.getControl(&quot;TypeInline&quot;).setState(1)
	If sEqType = &quot;latex&quot; Then oChildDialog.getControl(&quot;TypeLatex&quot;).setState(1)

	&apos; Set text values
	oChildDialog.getControl(&quot;LatexCode&quot;).setText(sEqCode)
	oChildDialog.getControl(&quot;LogString&quot;).setText( glb_sStatus )
	oChildDialog.getControl(&quot;Format&quot;).setText(sEqFormat)
	oChildDialog.getControl(&quot;GraphicDPI&quot;).setText(sEqDPI)

	&apos; Set editor font size
	oChildDialog.getControl(&quot;LatexCode&quot;).Model.FontName = glb_sEditorFontName

	&apos; Set editor font size
	oChildDialog.getControl(&quot;LatexCode&quot;).Model.FontHeight = glb_sEditorFontSize

	&apos; Set transparency
	If sEqTransp = &quot;TRUE&quot; Then
		oChildDialog.getControl(&quot;Transparency&quot;).setState(1)
	Else
		oChildDialog.getControl(&quot;Transparency&quot;).setState(0)
	End If
	
	&apos; Disable png and force svg if dvipng path is empty or if the compiler is xelatex or if dvipng does not exist
	If glb_sCompiler = &quot;xelatex&quot; Or oSystemInfo.DvipngPath = &quot;&quot; Or Not FileExists(oSystemInfo.DvipngPath) Then
		
		glb_sFormat = &quot;svg&quot;
		sEqFormat = glb_sFormat
		oChildDialog.getControl(&quot;Format&quot;).setText(&quot;svg&quot;)
		oChildDialog.getControl(&quot;Format&quot;).setEnable(FALSE)
	
	End If
			
	&apos; Disable svg and force png if dvisvgm path is empty or dvisvgm is not found
	If oSystemInfo.DvisvgmPath = &quot;&quot; Or Not FileExists(oSystemInfo.DvisvgmPath) Then
		
		glb_sFormat = &quot;png&quot; 
		sEqFormat = glb_sFormat
		oChildDialog.getControl(&quot;Format&quot;).setText(&quot;png&quot;)
		oChildDialog.getControl(&quot;Format&quot;).setEnable(FALSE)
	
	End If

	&apos; Disable dpi if svg image format
	If sEqFormat = &quot;svg&quot; Then 
		oChildDialog.getControl(&quot;GraphicDPI&quot;).setEnable(FALSE)
	Else 
		oChildDialog.getControl(&quot;GraphicDPI&quot;).setEnable(TRUE)
	End If

	&apos; Set the state of the tab buttons
	oChildDialog.getControl(&quot;General&quot;).Model.State=1
	oChildDialog.getControl(&quot;Arrays&quot;).Model.State=0
	oChildDialog.getControl(&quot;Alphabets&quot;).Model.State=0
	oChildDialog.getControl(&quot;Arrows&quot;).Model.State=0
	oChildDialog.getControl(&quot;Functions&quot;).Model.State=0
	oChildDialog.getControl(&quot;Relations&quot;).Model.State=0
	oChildDialog.getControl(&quot;Operations&quot;).Model.State=0
	oChildDialog.getControl(&quot;Misc&quot;).Model.State=0
	oChildDialog.Model.Step = 1

	&apos; Add listener
	oChildDialog.addWindowListener(CreateUnoListener(&quot;MainChildWindowListener_&quot;, &quot;com.sun.star.awt.XWindowListener&quot;))
	
	CreateInnerDialog = oChildDialog

End Function


&apos; Resize inner dialog window
&apos; This is called indirectly when resizing the outer base dialog      
Sub MainChildWindowListener_windowResized(ev)

	&apos; Inner dialog
	Dim oDialog as Variant
	oDialog = ev.Source
	
	&apos; Dialog size
	Dim aRect as Variant
	aRect = oDialog.getPosSize()
	
	&apos; Convert min width and height to pixels
	Dim minW as Integer, minH as Integer
	minW = DLG_MIN_WIDTH * convX
	minH = DLG_MIN_HEIGHT * convY

	&apos; Resize inner dialog if too narrow
	If aRect.Width &lt; minW Then

		aRect.Width = minW
		oDialog.setPosSize(0, 0, aRect.Width, aRect.Height, com.sun.star.awt.PosSize.SIZE)

	End If

	&apos; Resize inner dialog if too short
	If aRect.Height &lt; minH Then

		aRect.Height = minH
		oDialog.setPosSize(0, 0, aRect.Width, aRect.Height, com.sun.star.awt.PosSize.SIZE)

	End If

	Dim oWidget as Variant
	Dim X as Integer, Y as Integer, W as Integer, H as Integer

	&apos; Resize LaTeX code widget
	oWidget = oDialog.getControl(&quot;LatexCode&quot;)
	W = aRect.Width - (oDialog.Model.Width - oWidget.Model.Width ) * convX
	H = aRect.Height - (oDialog.Model.Height - oWidget.Model.Height ) * convY
	oWidget.setPosSize(0, 0, W, H, com.sun.star.awt.PosSize.SIZE)

	&apos; Position Image format widgets
	oWidget = oDialog.getControl(&quot;ImageformatFrame&quot;)
	X = aRect.Width - (oDialog.Model.Width - oWidget.Model.PositionX) * convX
	Y = oWidget.Model.PositionY * convY
	oWidget.setPosSize(X , Y, 0, 0, com.sun.star.awt.PosSize.POS)

	oWidget = oDialog.getControl(&quot;Format&quot;)
	X = aRect.Width - (oDialog.Model.Width - oWidget.Model.PositionX) * convX
	Y = oWidget.Model.PositionY * convY
	oWidget.setPosSize(X , Y, 0, 0, com.sun.star.awt.PosSize.POS)

	oWidget = oDialog.getControl(&quot;GraphicDPI&quot;)
	X = aRect.Width - (oDialog.Model.Width - oWidget.Model.PositionX) * convX
	Y = oWidget.Model.PositionY * convY
	oWidget.setPosSize(X , Y, 0, 0, com.sun.star.awt.PosSize.POS)

	oWidget = oDialog.getControl(&quot;DpiLabel&quot;)
	X = aRect.Width - (oDialog.Model.Width - oWidget.Model.PositionX) * convX
	Y = oWidget.Model.PositionY * convY
	oWidget.setPosSize(X , Y, 0, 0, com.sun.star.awt.PosSize.POS)

	oWidget = oDialog.getControl(&quot;Transparency&quot;)
	X = aRect.Width - (oDialog.Model.Width - oWidget.Model.PositionX) * convX
	Y = oWidget.Model.PositionY * convY
	oWidget.setPosSize(X , Y, 0, 0, com.sun.star.awt.PosSize.POS)

	&apos; Position Fontsize widgets
	oWidget = oDialog.getControl(&quot;FontsizeFrame&quot;)
	X = aRect.Width - (oDialog.Model.Width - oWidget.Model.PositionX) * convX
	Y = oWidget.Model.PositionY * convY
	oWidget.setPosSize(X , Y, 0, 0, com.sun.star.awt.PosSize.POS)

	oWidget = oDialog.getControl(&quot;Size&quot;)
	X = aRect.Width - (oDialog.Model.Width - oWidget.Model.PositionX) * convX
	Y = oWidget.Model.PositionY * convY
	oWidget.setPosSize(X , Y, 0, 0, com.sun.star.awt.PosSize.POS)

	oWidget = oDialog.getControl(&quot;PtsLabel&quot;)
	X = aRect.Width - (oDialog.Model.Width - oWidget.Model.PositionX) * convX
	Y = oWidget.Model.PositionY * convY
	oWidget.setPosSize(X , Y, 0, 0, com.sun.star.awt.PosSize.POS)

	&apos; Position Equation type widgets
	oWidget = oDialog.getControl(&quot;EquationtypeFrame&quot;)
	X = aRect.Width - (oDialog.Model.Width - oWidget.Model.PositionX) * convX
	Y = oWidget.Model.PositionY * convY
	oWidget.setPosSize(X , Y, 0, 0, com.sun.star.awt.PosSize.POS)

	oWidget = oDialog.getControl(&quot;TypeLatex&quot;)
	X = aRect.Width - (oDialog.Model.Width - oWidget.Model.PositionX) * convX
	Y = oWidget.Model.PositionY * convY
	oWidget.setPosSize(X , Y, 0, 0, com.sun.star.awt.PosSize.POS)

	oWidget = oDialog.getControl(&quot;TypeDisplay&quot;)
	X = aRect.Width - (oDialog.Model.Width - oWidget.Model.PositionX) * convX
	Y = oWidget.Model.PositionY * convY
	oWidget.setPosSize(X , Y, 0, 0, com.sun.star.awt.PosSize.POS)

	oWidget = oDialog.getControl(&quot;TypeInline&quot;)
	X = aRect.Width - (oDialog.Model.Width - oWidget.Model.PositionX) * convX
	Y = oWidget.Model.PositionY * convY
	oWidget.setPosSize(X , Y, 0, 0, com.sun.star.awt.PosSize.POS)

	&apos; Position Help button
	oWidget = oDialog.getControl(&quot;HelpButton&quot;)
	X = aRect.Width - (oDialog.Model.Width - oWidget.Model.PositionX) * convX
	Y = aRect.Height - (oDialog.Model.Height - oWidget.Model.PositionY ) * convY
	oWidget.setPosSize(X , Y, 0, 0, com.sun.star.awt.PosSize.POS)

	&apos; Position Preferences button
	oWidget = oDialog.getControl(&quot;PreferencesButton&quot;)
	X = aRect.Width - (oDialog.Model.Width - oWidget.Model.PositionX) * convX
	Y = aRect.Height - (oDialog.Model.Height - oWidget.Model.PositionY ) * convY
	oWidget.setPosSize(X , Y, 0, 0, com.sun.star.awt.PosSize.POS)

	&apos; Position Preamble button
	oWidget = oDialog.getControl(&quot;PreambleButton&quot;)
	X = aRect.Width - (oDialog.Model.Width - oWidget.Model.PositionX) * convX
	Y = aRect.Height - (oDialog.Model.Height - oWidget.Model.PositionY ) * convY
	oWidget.setPosSize(X , Y, 0, 0, com.sun.star.awt.PosSize.POS)

	&apos; Position LaTeX button
	oWidget = oDialog.getControl(&quot;LatexButton&quot;)
	X = aRect.Width - (oDialog.Model.Width - oWidget.Model.PositionX) * convX
	Y = aRect.Height - (oDialog.Model.Height - oWidget.Model.PositionY ) * convY
	oWidget.setPosSize(X , Y, 0, 0, com.sun.star.awt.PosSize.POS)

	&apos; Position state string
	oWidget = oDialog.getControl(&quot;LogString&quot;)
	X = oWidget.Model.PositionX * convX
	Y = aRect.Height - (oDialog.Model.Height - oWidget.Model.PositionY )* convY
	oWidget.setPosSize(X , Y, 0, 0, com.sun.star.awt.PosSize.POS)

End Sub


&apos; Force resizing when dialog is too small
Sub ForceResize()

	&apos; Minimum width and height
	Dim minW as Integer, minH as Integer
	minW = DLG_MIN_WIDTH * convX
	minH = DLG_MIN_HEIGHT * convY

	&apos; Actual dialog size
	Dim aSize as Variant
	aSize = oDlgBaseMain.getSize()

	&apos; If dialog too small, force resize
	If aSize.Width &lt; minW Or aSize.Height &lt; minH Then
		
		aSize.Width = minW
		aSize.Height = minH
		oDlgBaseMain.setPosSize(0, 0, aSize.Width, aSize.Height, com.sun.star.awt.PosSize.SIZE)

	End If

End Sub


&apos; Set image format and enable or disable dpi according to the image format
Sub Format_Mod()

	If oDlgMain.getControl(&quot;Format&quot;).getText() = &quot;svg&quot; Then
		oDlgMain.getControl(&quot;GraphicDPI&quot;).setEnable(FALSE)
	Else 
		oDlgMain.getControl(&quot;GraphicDPI&quot;).setEnable(TRUE)
	End If

End Sub


&apos; Set dpi value
Sub DPI_Mod()

	glb_sGraphicDPI = oDlgMain.getControl(&quot;GraphicDPI&quot;).getText()

End Sub


&apos; Set image transparency
Sub Trans_Mod()

	If oDlgMain.getControl(&quot;Transparency&quot;).getState() Then

		If GetDocumentType(ThisComponent) = &quot;swriter&quot; Then &apos; Writer document
			glb_sWriterTransparency = &quot;TRUE&quot;
		End If
	
		If GetDocumentType(ThisComponent) = &quot;simpress&quot; Then &apos; Impress document
			glb_sImpressTransparency = &quot;TRUE&quot;
		End If
	
		If GetDocumentType(ThisComponent) = &quot;sdraw&quot; Then &apos; Draw document
			glb_sDrawTransparency = &quot;TRUE&quot;
		End If

	Else

		If GetDocumentType(ThisComponent) = &quot;swriter&quot; Then &apos; Writer document
			glb_sWriterTransparency = &quot;FALSE&quot;
		End If
	
		If GetDocumentType(ThisComponent) = &quot;simpress&quot; Then &apos; Impress document
			glb_sImpressTransparency = &quot;FALSE&quot;
		End If
	
		If GetDocumentType(ThisComponent) = &quot;sdraw&quot; Then &apos; Draw document
			glb_sDrawTransparency = &quot;FALSE&quot;
		End If

	End If

End Sub



&apos; General tab button
Sub GeneralButton_Clicked(oEvent as Variant)

	&apos; Change the state of the tab buttons
	Dim oDialog as Variant
	oDialog = oEvent.Source.Context.View
	oDialog.getControl(&quot;General&quot;).Model.State=1
	oDialog.getControl(&quot;Arrays&quot;).Model.State=0
	oDialog.getControl(&quot;Alphabets&quot;).Model.State=0
